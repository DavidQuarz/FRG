{
  "openapi": "3.0.0",
  "info": {
    "description": "Documentation for FRG API REST",
    "version": "1.0.0",
    "title": "Quarz-Flask-API-FRG",
    "contact": {
      "email": "david.tia@student-cs.fr.com"
    },
    "license": {
      "name": "MIT",
      "url": "https://opensource.org/licenses/MIT"
    }
  },
  "servers": [
    {
      "url": "/"
    }
  ],
  "tags": [
    {
      "name": "Convert file to JSON",
      "description": "API for converting upload file into json file"
    }
  ],
  "paths": {
    "/api": {
      "get": {
        "tags": [
          "Convert file to JSON"
        ],
        "summary": "Returns \"Hello World !\"",
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/api/files": {
      "post": {
        "tags": [
          "Convert file to JSON"
        ],
        "summary": "Uplaod and convert file to JSON",
        "description": "",
        "operationId": "uploadFile",
        "consumes": [
          "application/json",
          "application/pdf",
          "image/gif",
          "image/jpeg",
          "image/png",
          "text/csv",
          "text/plain"
        ],
        "produces": [
          "application/json"
        ],
        "parameters" : [
          {
            "name": "file",
            "in" : "formData",
            "description" : "file to upload",
            "requiered" : "false",
            "type" : "file"
          }
        ],
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "id": {
        "properties": {
          "uuid": {
            "type": "string"
          }
        }
      }
    }
  }
}